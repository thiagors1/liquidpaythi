					"let host = pm.collectionVariables.get(\"host\");\r",
					"let login = pm.collectionVariables.get(\"login\");\r",
					"let senha = pm.collectionVariables.get(\"senha\");\r",
					"\r",
					"let body = JSON.stringify({\r",
					"    \"login\": login,\r",
					"    \"senha\": senha\r",
					"})\r",
					"\r",
					"const postRequest = {\r",
					"  url: host + '/api/login',\r",
					"  method: 'POST',\r",
					"  timeout: 0,\r",
					"  header: {\r",
					"    \"Content-Type\": \"application/json\",\r",
					"    \"Accept\": \"*/*\"\r",
					"  },\r",
					"  body: body\r",
					"};\r",
					"pm.sendRequest(postRequest, function (err, res) {\r",
					"    if (err) {\r",
					"        console.log(\"Error: \", err);\r",
					"    } else if (res.code === 200) { // Verifica se a resposta é 200 OK\r",
					"        console.log(\"Login Response: \", res);\r",
					"        \r",
					"        // Captura o token do corpo da resposta (ajuste conforme a estrutura do seu JSON)\r",
					"        let jsonData = res.json();\r",
					"        let token = jsonData.token; // Supondo que o token está no campo 'token'\r",
					"\r",
					"        // Armazena o token, login e senha em variáveis de coleção\r",
					"        pm.collectionVariables.set('token', token);\r",
					"        console.log(\"Token, login e senha armazenados com sucesso.\", token);\r",
					"    } else {\r",
					"        console.log(\"Falha no login. Status: \", res.code);\r",
					"    }\r",
					"});"
				]
			}
